<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Bee Incubators -->



	<RecipeDef>
		<defName>GR_CreateBearbeeIncubator</defName>
		<label>bearbee incubator</label>
		<description>Fill an incubator with genes from a bear and bees</description>
		<jobString>Filling Incubator</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>

			<li>GR_BeesGenePod</li>				
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>GR_EmptyIncubator</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_BearGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_BeeGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>

			<thingDefs>
				<li>GR_EmptyIncubator</li>
				<li>GR_BearGenetic</li>
				<li>GR_BeeGenetic</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_BearbeeIncubator>1</GR_BearbeeIncubator>
		</products>
		<researchPrerequisite>GR_BeeGeneticEngineering</researchPrerequisite>
		<skillRequirements>
			<Intellectual>5</Intellectual>
		</skillRequirements>
	</RecipeDef>
	
