<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 

  
  <HediffDef>
    <defName>RB_Stamina</defName>
    <label>increased stamina (from honey)</label>
    <makesSickThought>false</makesSickThought>
    <tendable>false</tendable>
	<isBad>false</isBad>
    <defaultLabelColor>(255,95,95)</defaultLabelColor>
	<hediffClass>HediffWithComps</hediffClass>
    <initialSeverity>1.0</initialSeverity>
	<maxSeverity>1.0</maxSeverity>
    <comps>
    
	  <li Class="RimBeesGeneticPatch.HediffCompProperties_WhileHavingThoughts">
        <thoughtDefs>
			<li>RB_AteGeneticHoney</li>
			<li>RB_AteGeneticHoneyIngredient</li>
		</thoughtDefs>
      </li>
    </comps>
    <stages>
     
      <li>   
	  <painOffset>-0.10</painOffset>
	  <painFactor>0.9</painFactor>

         <statOffsets>
            <ArmorRating_Blunt>0.05</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.05</ArmorRating_Sharp>

          </statOffsets>
      </li>
	  
	 
     
    </stages>
  </HediffDef>
  
 
  
 

</Defs>