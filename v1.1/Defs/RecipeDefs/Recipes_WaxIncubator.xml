<?xml version="1.0" encoding="utf-8" ?>
<Defs>



	<RecipeDef>
		<defName>GR_BaseIncubatorWaxRecipe</defName>
		<label>manufacture empty wax incubator</label>
		<description>Manufacture an empty incubator using Genewax and an organic growth compound.</description>
		<jobString>Manufacturing empty wax incubator</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>800</workAmount>
		<recipeUsers>
			<li>GR_GeneticExtractionTable</li>	 
			<li>GR_AlphaSerumExtractionTable</li>	  

		</recipeUsers>	
		<ingredients>
			<li>
				<filter>

					<thingDefs>
						<li>RB_Genewax</li>

					</thingDefs>
				</filter>
				<count>50</count>
			</li>
			<li>
				<filter>

					<thingDefs>
						<li>GR_OrganicPulp</li>

					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>

			<thingDefs>
				<li>RB_Genewax</li>
				<li>GR_OrganicPulp</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_EmptyIncubator>1</GR_EmptyIncubator>
		</products>

	</RecipeDef>







</Defs>