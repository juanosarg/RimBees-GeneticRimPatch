<Defs>



	<!--========== Genetic Tier: Basic (bought from merchants or crafted in the Genetic Table) ==========-->

	<!--=============== Organic ====================-->

	<ThingDef ParentName="RB_BeeBase" Name="RB_OrganicBeeBase" Abstract="True" >
		<description>Organic bees are genetically modified bees that extract cellulose fibers from nearby plants and convert it into organic pulp, which they store on their honeycombs.\n\nBee Traits:\n\nProduction time: 24 hours\nNocturnal: No\nPluviophile: No\nUnusual plants needed: No\nMinimum Temperature: 0ºC\nMaximum temperature: 30ºC</description>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>   
		<tradeability>Buyable</tradeability>
		<statBases>
			<MarketValue>100</MarketValue>
		</statBases>
		<comps>			
			<li Class="RimBees.CompProperties_Bees">
				<species>Organic</species>
				<comb>RB_Organic_Honeycomb</comb>
				<combtimedays>1</combtimedays>
				<nocturnal>false</nocturnal>
				<pluviophile>false</pluviophile>
				<weirdplantneeded>no</weirdplantneeded>
				<tempMin>0</tempMin>
				<tempMax>30</tempMax>
			</li>   
		</comps>
	</ThingDef>

	<ThingDef ParentName="RB_OrganicBeeBase">
		<defName>RB_Bee_Organic_Drone</defName>
		<label>organic drone</label>
		<graphicData>
			<texPath>Things/Item/Bees/RB_Bee_Organic_Drone</texPath>
		</graphicData> 		
	</ThingDef>
	<ThingDef ParentName="RB_OrganicBeeBase">
		<defName>RB_Bee_Organic_Queen</defName>
		<label>organic queen</label>
		<graphicData>
			<texPath>Things/Item/Bees/RB_Bee_Organic_Queen</texPath>
		</graphicData> 		
	</ThingDef>

